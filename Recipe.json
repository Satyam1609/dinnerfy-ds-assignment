   "Serve_2":{
      "recipeSource":"My Kitchen",
      "lastUpdated":"2021-09-20",
      "instructions":[
         {
            "stepNumber":1,
            "stageTitle":"Preparation",
            "stepType":"prep",
            "instruction":"Wash and soak 1 cup of basmati rice in water for 30 minutes. Chop 1 onion, 1 tomato, 1 carrot, and 1 potato into small pieces. Grind 1 inch ginger and 4 garlic cloves into a paste. Soak 10-12 cashews in water for 10 minutes.",
            "time":30,
            "tool":"bowl, knife, blender"
         },
         {
            "stepNumber":2,
            "stageTitle":"Cooking",
            "stepType":"cook",
            "instruction":"Heat 2 tablespoons of oil in a pan. Add 1 bay leaf, 2 cloves, 2 cardamom pods, and 1 cinnamon stick. Add the chopped onion and saut√© until golden brown. Add the ginger-garlic paste and saut√© for 1 minute. Add the chopped vegetables and saut√© for 2-3 minutes. Add 1 teaspoon of biryani masala and mix well. Drain the soaked rice and add it to the pan. Add 2 cups of water and salt to taste. Mix well and cover the pan with a lid. Cook on low flame for 15-20 minutes or until the rice is cooked.",
            "time":30,
            "tool":"pan, spatula, lid"
         },
         {
            "stepNumber":3,
            "stageTitle":"Serving",
            "stepType":"serve",
            "instruction":"Garnish with chopped coriander leaves and fried onions. Serve hot with raita or pickle.",
            "time":0,
            "tool":"plate, spoon"
         }
      ]
